{
  "compilerOptions": {
    "baseUrl": "./src/client/",
    "jsx": "preserve",
    "module": "esnext",
    "target": "es2016",
    "strict": true,
    "allowUmdGlobalAccess": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "./**/*"
, "src/pages/spotify/.index.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
